            fx:controller="io.github.dylmeadows.eontimer.controller.timer.TimerControlController">
    <center>
        <TabPane fx:id="timerTabPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
                 minHeight="-Infinity" minWidth="-Infinity" styleClass="floating" tabClosingPolicy="UNAVAILABLE">
            <tabs>
                <Tab fx:id="gen5Tab" text="5">
                    <content>
                        <AnchorPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
                            <fx:include fx:id="gen5Timer" source="Gen5TimerPane.fxml"
                                        AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                                        AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0"/>
                        </AnchorPane>
                    </content>
                </Tab>
                <Tab fx:id="gen4Tab" text="4">
                    <content>
                        <AnchorPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
                            <fx:include fx:id="gen4Timer" source="Gen4TimerPane.fxml"
                                        AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                                        AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0"/>
                        </AnchorPane>
                    </content>
                </Tab>
                <Tab fx:id="gen3Tab" text="3">
                    <content>
                        <AnchorPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
                            <fx:include fx:id="gen3Timer" source="Gen3TimerPane.fxml"
                                        AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                                        AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0"/>
                        </AnchorPane>
                    </content>
                </Tab>
                <Tab fx:id="customTab" text="C">
                    <content>
                        <AnchorPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
                            <fx:include fx:id="customTimer" source="CustomTimer.fxml"
                                        AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                                        AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0"/>
                        </AnchorPane>
                    </content>
                </Tab>
            </tabs>
        </TabPane>
    </center>
</BorderPane>
